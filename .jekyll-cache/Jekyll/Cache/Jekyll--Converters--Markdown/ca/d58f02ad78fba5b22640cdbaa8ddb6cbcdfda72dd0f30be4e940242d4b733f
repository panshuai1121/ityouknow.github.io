    # æ‰§è¡ŒSQLè¯­å¥
    cursor_test.execute(sql)
    # æäº¤åˆ°æ•°æ®åº“æ‰§è¡Œ
    conn.commit()
except:
    # å‘ç”Ÿé”™è¯¯æ—¶å›æ»š
    conn.rollback()

# å…³é—­æ•°æ®åº“è¿æ¥
conn.close()
</code></pre></div></div>
<h3 id="å…­åˆ é™¤æ“ä½œ">å…­ã€åˆ é™¤æ“ä½œ</h3>
